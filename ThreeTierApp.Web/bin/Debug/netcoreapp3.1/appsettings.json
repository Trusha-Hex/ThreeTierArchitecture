{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=test;User=root;Password=1234;Port=3307;",
    "Redis": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "NLog": {
    "targets": {
      "file": {
        "type": "File",
        "fileName": "log.txt",
        "layout": "${longdate} ${level} ${message} ${exception}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "file"
      }
    ]
  },
  "AllowedHosts": "*"
}
